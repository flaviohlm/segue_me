	<queryString>
		<![CDATA[SELECT encontro_equipe.id AS encontro_equipe_id,
     encontro.descricao AS encontro_descricao,
     encontro.data_realizacao_inicio AS encontro_data_realizacao_inicio,
     encontro.data_realizacao_fim AS encontro_data_realizacao_fim,
     equipe.descricao AS equipe_descricao,
     paroquia.descricao AS paroquia,
     (SELECT nome FROM segueme.participante p, segueme.seguidor s, segueme.encontro_equipe_integrantes e WHERE p.id = s.participante_id AND s.id = e.seguidor_id AND e.coordenador = true AND p.sexo = 'M' AND e.encontro_equipe_id = encontro_equipe.id) AS COORDENADOR,
     (SELECT nome FROM segueme.participante p, segueme.seguidor s, segueme.encontro_equipe_integrantes e WHERE p.id = s.participante_id AND s.id = e.seguidor_id AND e.coordenador = true AND p.sexo = 'F' AND e.encontro_equipe_id = encontro_equipe.id) AS COORDENADORA
FROM segueme.encontro_equipe encontro_equipe
INNER JOIN segueme.equipe equipe ON encontro_equipe.equipe_id = equipe.id
INNER JOIN segueme.encontro encontro ON encontro.id = encontro_equipe.encontro_id
INNER JOIN segueme.encontro_paroquia encontro_paroquia ON encontro.id = encontro_paroquia.encontro_id
INNER JOIN segueme.paroquia paroquia ON encontro_paroquia.paroquia_id = paroquia.id

WHERE encontro.id = $P{ENCONTRO_ID}
ORDER BY equipe.descricao]]>
	</queryString>
